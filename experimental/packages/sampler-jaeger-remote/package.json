{
  "name": "@opentelemetry/sampler-jaeger-remote",
  "version": "0.209.0",
  "description": "Jaeger Remote Sampler",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/sampler-jaeger-remote",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    }
  },
  "repository": "open-telemetry/opentelemetry-js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsdown",
    "clean": "rm -rf build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "test": "nyc mocha 'test/**/*.test.ts' --exclude 'test/browser/**/*.ts'",
    "version": "node ../../../scripts/version-update.js",
    "watch": "tsc -w",
    "prewatch": "npm run precompile",
    "peer-api-check": "node ../../../scripts/peer-api-check.js",
    "align-api-deps": "node ../../../scripts/align-api-deps.js"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "metrics",
    "stats"
  ],
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@opentelemetry/sdk-trace-base": "2.3.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.9.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.6.5",
    "@types/sinon": "17.0.4",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "11.7.5",
    "nyc": "17.1.0",
    "sinon": "18.0.1",
    "ts-loader": "9.5.4",
    "typescript": "5.0.4"
  },
  "sideEffects": false
}
