{
  "name": "@opentelemetry/instrumentation-react-native-navigation",
  "version": "1.25.1",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "description": "Instrumentation library that captures telemetry data during navigation changes for React Native projects.",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/opentelemetry-instrumentation-react-native-navigation",
  "license": "Apache-2.0",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-telemetry/opentelemetry-js.git",
    "directory": "packages/opentelemetry-instrumentation-react-native-navigation"
  },
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsc --build",
    "version": "node ../../../scripts/version-update.js",
    "clean": "tsc --build --clean",
    "precompile": "cross-var lerna run version --scope $npm_package_name --include-dependencies",
    "prewatch": "npm run precompile",
    "peer-api-check": "node ../../../scripts/peer-api-check.js",
    "align-api-deps": "node ../../../scripts/align-api-deps.js",
    "test": "nyc ts-mocha --project tsconfig.json",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../../",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch": "tsc --build --watch"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "browser",
    "tracing",
    "profiling",
    "metrics",
    "stats",
    "monitoring"
  ],
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "dependencies": {
    "@opentelemetry/api": "1.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2"
  },
  "devDependencies": {
    "@opentelemetry/sdk-trace-base": "1.25.1",
    "@react-navigation/native": "5.9.8",
    "@testing-library/dom": "10.2.0",
    "@testing-library/react": "16.0.0",
    "@testing-library/react-native": "12.5.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.6.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@types/sinon": "10.0.13",
    "chai": "5.1.1",
    "codecov": "3.8.3",
    "cross-var": "1.1.0",
    "lerna": "6.6.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "react-native-navigation": "7.40.1",
    "react-test-renderer": "18.2.0",
    "sinon": "15.0.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4"
  }
}
